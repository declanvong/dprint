{
  "incremental": true,
  "indentWidth": 2,
  "lineWidth": 160,
  "rustfmt": {
    "imports_granularity": "Item"
  },
  "includes": [
    "**/*.{ts,tsx,js,jsx,json,md,html,scss,css,toml,rs}"
  ],
  "excludes": [
    "website/playground/build",
    "scripts/build-website",
    "**/node_modules",
    "**/dist",
    "**/target",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.65.1.wasm",
    "https://plugins.dprint.dev/json-0.14.1.wasm",
    "https://plugins.dprint.dev/markdown-0.12.2.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/prettier-0.6.1.exe-plugin@2d54d5f8dbdeb087762fadc4840c688327f5deab447c94b32ca3850106db9224",
    "https://plugins.dprint.dev/rustfmt-0.6.1.exe-plugin@99b89a0599fd3a63e597e03436862157901f3facae2f0c2fbd0b9f656cdbc2a5"
  ]
}
